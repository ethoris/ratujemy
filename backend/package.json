{
    "name": "cms-backend",
    "version": "1.0.0",
    "description": "CMS Backend with Node.js, Express, Sequelize, PostgreSQL, Redis, Stripe, SendGrid, and Passport.js",
    "main": "index.js",
    "scripts": {
        "start": "node server.js",
        "dev": "kill-port 4000 && nodemon server.js",
        "migrate": "sequelize db:migrate",
        "seed": "sequelize db:seed:all",
        "test": "cross-env NODE_ENV=test jest --runInBand"
    },
    "dependencies": {
        "@sendgrid/mail": "^7.7.0",
        "bcrypt": "^5.1.1",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.20.3",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-rate-limit": "^7.5.0",
        "express-validator": "^7.2.1",
        "helmet": "^7.2.0",
        "jsonwebtoken": "^9.0.0",
        "multer": "^1.4.5-lts.1",
        "nodemailer": "^6.10.0",
        "passport": "^0.6.0",
        "passport-facebook": "^3.0.0",
        "passport-google-oauth20": "^2.0.0",
        "pg": "^8.13.1",
        "pg-hstore": "^2.3.4",
        "rate-limit-redis": "^1.7.0",
        "redis": "^4.7.0",
        "sequelize": "^6.37.5",
        "stripe": "^12.0.0",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1",
        "winston": "^3.17.0"
    },
    "devDependencies": {
        "jest": "^29.7.0",
        "kill-port": "^2.0.1",
        "nodemon": "^3.0.1",
        "sequelize-cli": "^6.6.2",
        "supertest": "^6.3.4"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "license": "MIT"
}
